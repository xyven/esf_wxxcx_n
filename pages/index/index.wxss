/**index.wxss**/
.sv{
  position: absolute;
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
}

.agent-list{
  width: 100%;
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
  /*float: left;*/
  /*background-color: cadetblue;
  justify-content: space-around;*/
}

.agent-item{
  display: flex;
  flex-direction: row;
  margin-top: 16rpx;
  background-color: #fff;
  border-radius: 10rpx;
}

.agent-img{
  width: 120rpx;
  height: 120rpx;
  margin-left: 10rpx;
  border-radius: 120rpx;
  background: #aaaaaa;
}

.agent-info-txt1{
  display: flex;
  flex-direction: column;
  margin-left: 10rpx;
  width: 300rpx;
  height: 120rpx;
}

.agent-info-txt2{
  display: flex;
  flex-direction: column;
  margin-left: 0rpx;
}

.agent-info1{
  font-size: 12px;
  line-height: 16px;
  white-space: nowrap;
  color: black;  
  text-overflow: ellipsis;
  overflow: hidden;
}

.agent-info2{
  font-size: 12px;
  color: #929292;
  line-height: 16px;  
}
